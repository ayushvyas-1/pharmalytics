      "order": 0
    },
    {
      "id": "slide-2-1",
      "presentationId": "presentation-2",
      "title": "Slide 2: Treatment Indications",
      "content": "Indicated for adults with T2DM with inadequate control on metformin",
      "imageUrl": "/placeholder.svg?height=600&width=800&text=Treatment+Agenda",
      "order": 1
    },
    {
      "id": "slide-2-2",
      "presentationId": "presentation-2",
      "title": "Slide 3: Pharmacodynamics",
      "content": "Increases glucose excretion and improves insulin sensitivity",
      "imageUrl": "/placeholder.svg?height=600&width=800&text=Company+Profile",
      "order": 2
    },
    {
      "id": "slide-2-3",
      "presentationId": "presentation-2",
      "title": "Slide 4: Administration Protocol",
      "content": "Once-daily oral tablet, 25mg with or without food",
      "imageUrl": "/placeholder.svg?height=600&width=800&text=Drug+Mechanism",
      "order": 3
    },
    {
      "id": "slide-2-4",
      "presentationId": "presentation-2",
      "title": "Slide 5: Adverse Reactions",
      "content": "Monitor for hypoglycemia (2%), UTIs (4%), and dehydration (1%)",
      "imageUrl": "/placeholder.svg?height=600&width=800&text=Clinical+Benefits",
